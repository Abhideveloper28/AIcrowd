{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        corejs: '3',
        "targets": {
          "browsers": "> 1%",
          "uglify": true,
          "node": "current"
        },
        "useBuiltIns": "usage"
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true,
        "corejs": 3
      }
    ],
    "syntax-dynamic-import",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}
